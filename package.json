{
	"name": "bin-rw",
	"description": "Simple, tiny, binary-driven encoding format",
	"packageManager": "yarn@4.0.1",
	"type": "module",
	"version": "0.1.0",
	"author": {
		"name": "Denis Cristea",
		"email": "didinele.dev@gmail.com"
	},
	"exports": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/didinele/bin-rw.git"
	},
	"bugs": {
		"url": "https://github.com/didinele/bin-rw/issues"
	},
	"homepage": "https://github.com/didinele/bin-rw",
	"scripts": {
		"build": "tsup && tsc",
		"lint": "eslint src --ext .ts && prettier --check .",
		"test": "vitest run",
		"test:watch": "vitest",
		"format": "prettier --write .",
		"update": "yarn upgrade-interactive",
		"postinstall": "is-ci || husky install || true"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-angular": "^18.1.0",
		"@types/is-ci": "^3",
		"@vitest/coverage-v8": "^0.34.6",
		"eslint": "^8.53.0",
		"eslint-config-neon": "^0.1.57",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.0.2",
		"prettier": "^3.0.3",
		"prettier-eslint": "^16.1.2",
		"tsup": "^7.2.0",
		"turbo": "^1.10.16",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"dependencies": {
		"discord.js": "^14.14.1"
	}
}
